<%- include('../partials/header') %>

<h2><%= title %></h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Country</th>
      <th>Ranking</th>
      <th>UTR</th>
      <th></th>
    </tr>
  </thead>
  
  <tbody>
    <% players.forEach(function(p) { %>
      <tr>
        <td><%= p.name %></td>
        <td><%= p.age %></td>
        <td><%= p.country %></td>
        <td><%= p.ranking %></td>
        <td><%= p.utr %></td>
        <td><a href="/players/<%= p._id %>">Info</a></td>
      </tr>
    </tbody>
  <% }) %>
</table>

<%- include('../partials/footer') %>